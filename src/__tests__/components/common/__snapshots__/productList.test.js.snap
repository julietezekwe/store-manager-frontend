                }
              >
                <i
                  className="far fa-trash-alt trigger delete"
                  id="show"
                  onClick={[Function]}
                />
                <dialog>
                  <div
                    className="modal-content"
                  >
                    <span
                      className="close-button"
                      id="close"
                      onClick={[Function]}
                    >
                      ×
                    </span>
                    <div
                      className="modal-body"
                    >
                      <div
                        className="form"
                      >
                        <form
                          className="login"
                          onSubmit={[Function]}
                        >
                          <div
                            className="card"
                          >
                            <div
                              className="product-detail"
                            >
                              <h3
                                style={
                                  Object {
                                    "textAlign": "center",
                                  }
                                }
                              >
                                Do you want to delete this product?
                              </h3>
                              <button
                                className="button delete-btn"
                                onClick={[Function]}
                              >
                                Delete
                              </button>
                            </div>
                          </div>
                        </form>
                      </div>
                    </div>
                  </div>
                </dialog>
              </Form>
            </Connect(Form)>
          </div>
        </div>
      </div>
    </div>
  </ProductList>
</Provider>
`;
